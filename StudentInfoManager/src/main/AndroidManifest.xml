<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.yueyue.studentinfomanager">

    <application
        android:name=".base.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".modules.main.ui.MainActivity"
            android:launchMode="singleTask"/>
        <activity android:name=".modules.login.ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".modules.search.ui.SearchActivity">
        </activity>
        <activity android:name=".modules.edit.ui.EditActivity">
        </activity>
        <activity android:name=".modules.personal.ui.PersonalActivity">
        </activity>
    </application>

</manifest>